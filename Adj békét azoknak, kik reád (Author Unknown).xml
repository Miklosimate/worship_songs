<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-02-28T13:53:14">
  <properties>
    <titles>
      <title>Adj békét azoknak, kik reád</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Adj békét azoknak, kik reád várnak,<br/>hogy igazmondóknak bizonyuljanak prófétáid!<br/>Hallgasd meg, Uram, szolgádat,<br/>s népednek, Izraelnek könyörgését.</lines>
    </verse>
  </lyrics>
</song>
