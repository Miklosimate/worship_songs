<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.0.2" modifiedIn="OpenLP 3.0.2" modifiedDate="2024-02-05T17:31:04">
  <properties>
    <titles>
      <title>Háztetőkről</title>
      <title>Rooftops</title>
    </titles>
    <verseOrder>c1 b1</verseOrder>
    <authors>
      <author>unknown</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1">
      <lines>Rólad zeng énekem, háztetőkről hirdetem<br/>Hogy bennem élsz, bennem élsz.</lines>
    </verse>
    <verse name="b1">
      <lines>Kezeim feléd nyújtom, Jézus,<br/>Egyedül Te vagy a végtelen Király,<br/>a végtelen Király!</lines>
    </verse>
  </lyrics>
</song>
