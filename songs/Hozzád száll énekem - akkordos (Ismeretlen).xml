<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-03-31T15:06:32">
  <properties>
    <titles>
      <title>Hozzád száll énekem - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>G--------C----------D<br/>Hozzád száll az énekem,</lines>
    </verse>
    <verse name="v1b">
      <lines>G--------C----------D<br/>Olyan jó Téged dícsérni!</lines>
    </verse>
    <verse name="v1c">
      <lines>G-------C---------D<br/>Bűnös volt az életem,</lines>
    </verse>
    <verse name="v1d">
      <lines>G------------C--------------D<br/>Eljöttél, hogy megments engem.</lines>
    </verse>
    <verse name="v1e">
      <lines>G-----------C--------D---------C----G<br/>A mennyből jöttél közénk, hogy út legyél,</lines>
    </verse>
    <verse name="v1f">
      <lines>---------C------D-----C--G-<br/>Feláldoztad magad a bűnömért.</lines>
    </verse>
    <verse name="v1g">
      <lines>----------Am--------D----------Em-------C<br/>A Golgotán meghaltál, harmadnap feltámadtál,</lines>
    </verse>
    <verse name="v1h">
      <lines>-------D-----G-C-D-C<br/>Uralkodsz mindörökké!</lines>
    </verse>
  </lyrics>
</song>
