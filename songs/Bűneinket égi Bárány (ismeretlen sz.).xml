  <properties>
    <titles>
      <title>Bűneinket égi Bárány</title>
    </titles>
    <authors>
      <author>ismeretlen sz.</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Bűneinket égi Bárány,<br/>Szent vérednek drága árán mind elveszed.</lines>
    </verse>
    <verse name="v1b">
      <lines>1. Irgalmazz nekünk!<br/>2. Irgalmazz nekünk!<br/>3. Adj békét nekünk!</lines>
    </verse>
  </lyrics>
</song>
