    <verseOrder>v1 c1</verseOrder>
    <authors>
      <author type="words">Twila Paris</author>
      <author type="music">Twila Paris</author>
      <author type="translation">Németh S. Judit</author>
    </authors>
    <songbooks>
      <songbook name="Szent András énekfüzet" entry="64"/>
    </songbooks>
