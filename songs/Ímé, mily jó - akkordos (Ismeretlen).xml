<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-02-10T15:13:29">
  <properties>
    <titles>
      <title>Ímé, mily jó - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="c1a">
      <lines>Em-Am-------------------Em<br/>Í    mé, mily jó, gyönyörűséges</lines>
    </verse>
    <verse name="c1b">
      <lines>--C---------D---Em<br/>A testvérek egysége!</lines>
    </verse>
    <verse name="v1a">
      <lines>-------A---------------<br/>Mint a drága olaj a fejen,</lines>
    </verse>
    <verse name="v1b">
      <lines>--------D--------------<br/>Mint a Hermon harmatja,</lines>
    </verse>
    <verse name="v1c">
      <lines>-----------A--------------<br/>Csak oda küld áldást az Úr,</lines>
    </verse>
    <verse name="v1d">
      <lines>---D-------A---<br/>És életet örökké!</lines>
    </verse>
    <verse name="c2a">
      <lines>Em-Am-----------Em------<br/>Hi    ney mah tov umah naim</lines>
    </verse>
    <verse name="c2b">
      <lines>C---------D-------Em----<br/>Shevet achim gam yachad!</lines>
    </verse>
    <verse name="v2a">
      <lines>------------A----------------<br/>Hiney mah tov, hiney mah tov,</lines>
    </verse>
    <verse name="v2b">
      <lines>----D--A-<br/>Ná-ná-náj</lines>
    </verse>
  </lyrics>
</song>
