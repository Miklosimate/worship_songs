<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-12-22T14:04:23">
  <properties>
    <titles>
      <title>Mi vagyunk az Isten népe</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="14"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Mi vagyunk az Isten népe,<br/>* jó Pásztorunk táplál minket.</lines>
    </verse>
    <verse name="v2">
      <lines>Mennyire jólesik nékem,<br/>* tégedet, Uram, hálával dicsérnem.</lines>
    </verse>
    <verse name="v3">
      <lines>Bocsásd meg, Uram bűnömet,<br/>* vétkeim terhét vedd el rólam.</lines>
    </verse>
  </lyrics>
</song>
