<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-02-10T15:45:44">
  <properties>
    <titles>
      <title>Egy a test - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>C----Fmaj7-C---Fmaj7<br/>Egy a test,   egy a Lélek</lines>
    </verse>
    <verse name="v1b">
      <lines>C---Fmaj7--------G--------<br/>Elhívásunk egy reményre szól</lines>
    </verse>
    <verse name="v1c">
      <lines>C----Fmaj7-C-Fmaj7<br/>Egy az Úr, egy a hit,</lines>
    </verse>
    <verse name="v1d">
      <lines>C----Fmaj7------------G--<br/>Egy a keresztség, Jézusunk.</lines>
    </verse>
    <verse name="c1a">
      <lines>Am7--Em7----Am7--------Em7<br/>Egy az Isten,  mindeneknek Atyja,</lines>
    </verse>
    <verse name="c1b">
      <lines>Fmaj7-------------G<br/>Mindenek fölött Ő áll!</lines>
    </verse>
    <verse name="c1c">
      <lines>Am7----Em7---Am7--------Em7<br/>Mindent áthat, mindenben Ő van,</lines>
    </verse>
    <verse name="c1d">
      <lines>Fmaj7---------------------G<br/>Testvérek szeressük egymást!</lines>
    </verse>
  </lyrics>
</song>
