<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-03-10T15:22:27">
  <properties>
    <titles>
      <title>Élet van Tebenned - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>--G-------------<br/>Élet van Tebenned</lines>
    </verse>
    <verse name="v1b">
      <lines>--Em------------<br/>Erő van Tebenned</lines>
    </verse>
    <verse name="v1c">
      <lines>-----C---------------------G-D<br/>Remény van Tebenned, Ó Jézus!</lines>
    </verse>
    <verse name="v1d">
      <lines>C-------------D------G<br/>Így dícsérelek életemmel</lines>
    </verse>
    <verse name="v1e">
      <lines>---C--------D----------G-<br/>És áldalak minden erőmmel!</lines>
    </verse>
    <verse name="v1f">
      <lines>---D------G-----E-------Am--<br/>Az életemmel, minden erőmmel,</lines>
    </verse>
    <verse name="v1g">
      <lines>-------F-----------------D-C-D<br/>Csak benned van reménységem!</lines>
    </verse>
  </lyrics>
</song>
