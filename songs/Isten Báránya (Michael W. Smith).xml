    <titles>
      <title>Isten Báránya</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>Michael W. Smith</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Alleluja, alleluja, uralkodik a mi Istenünk<br/>Szent vagy, szent vagy, Mindenható szent Isten vagy,<br/>Te vagy méltó, Te vagy méltó, Isten báránya</lines>
    </verse>
  </lyrics>
