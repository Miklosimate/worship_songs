<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-03-17T15:17:01">
  <properties>
    <titles>
      <title>Véred megnyitja szemem - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>---G------------------C2----------<br/>Véred megnyitja szemem - épp most</lines>
    </verse>
    <verse name="v1b">
      <lines>----G-------------C2-----------<br/>Véred átkokat tör meg - épp most</lines>
    </verse>
    <verse name="v1c">
      <lines>--Am7-------------C2----------<br/>Véred gyógyít, felépít - épp most</lines>
    </verse>
    <verse name="v1d">
      <lines>----G-------------D/F#--------<br/>Véred láncokat szakít - épp most</lines>
    </verse>
    <verse name="c1a">
      <lines>---Am7-------C2-<br/>És én ma is hiszem</lines>
    </verse>
    <verse name="c1b">
      <lines>----------------G---------<br/>Hogy Benned nincs változás</lines>
    </verse>
    <verse name="c1c">
      <lines>-----D/F#---------------<br/>Ugyanaz vagy mindörökké</lines>
    </verse>
    <verse name="c1d">
      <lines>---Am7--------C2<br/>És én ma is hiszem</lines>
    </verse>
    <verse name="c1e">
      <lines>-----------------G----------D/F#<br/>Hogy a te szent véred elég nekem.</lines>
    </verse>
    <verse name="v2a">
      <lines>---G------------------C2----------<br/>Véred megtört szívekre hull - épp most</lines>
    </verse>
    <verse name="v2b">
      <lines>----G-------------C2-----------<br/>Véred segít megbocsátanom - épp most</lines>
    </verse>
    <verse name="v2c">
      <lines>--Am7-------------C2----------<br/>Véred értelmem mossa át - épp most</lines>
    </verse>
    <verse name="v2d">
      <lines>----G-------------D/F#--------<br/>Véred adhat feltámadást - épp most</lines>
    </verse>
    <verse name="b1a">
      <lines>---------Em-------------<br/>Te vagy Isten a mennyben </lines>
    </verse>
    <verse name="b1b">
      <lines>D2/F#------------<br/>Az Úr minden felett.</lines>
    </verse>
    <verse name="b1c">
      <lines>------G-----------------------A4<br/>A bűn törvénye helyett a szabadság.</lines>
    </verse>
  </lyrics>
