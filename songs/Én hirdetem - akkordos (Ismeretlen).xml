  <properties>
    <titles>
      <title>Én hirdetem - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>A--Amaj7-D-E--E7------A4--A-<br/>Én hirdetem az élő Úr dicséretét!</lines>
    </verse>
    <verse name="v1b">
      <lines>A-Amaj7----D--E------E7-A4-A<br/>Értem halt meg, feláldozta életét.</lines>
    </verse>
    <verse name="c1a">
      <lines>-----------D-E<br/>Örökké ő az Úr,</lines>
    </verse>
    <verse name="c1b">
      <lines>--------------A4-A-F#m<br/>Mint bárány ül a trónon!</lines>
    </verse>
    <verse name="c1c">
      <lines>-----------------Hm-E7<br/>Meghajtom térdemet és</lines>
    </verse>
    <verse name="c1d">
      <lines>------------A4-A<br/>Őt magasztalom!</lines>
    </verse>
  </lyrics>
</song>
