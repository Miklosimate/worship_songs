  <properties>
    <titles>
      <title>Halljad, leányom, és figyelj</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - ünnepek" entry="253"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Halljad, leányom, és figyelj, * füledet hajlítsd a szavamra!</lines>
    </verse>
    <verse name="v2">
      <lines>Íme, jön a Vőlegény: * menjetek az Úr Krisztus elé!</lines>
    </verse>
    <verse name="v3">
      <lines>Ifjak és szüzek, * dicsérjétek az Úr nevét!</lines>
    </verse>
  </lyrics>
</song>
