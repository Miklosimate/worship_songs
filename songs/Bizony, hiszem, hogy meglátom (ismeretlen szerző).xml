  <properties>
    <titles>
      <title>Bizony, hiszem, hogy meglátom</title>
    </titles>
    <verseOrder>v1</verseOrder>
    <authors>
      <author>ismeretlen szerző</author>
    </authors>
    <songbooks>
      <songbook name="Zöld könyv" entry="399"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Bizony, hiszem, hogy meglátom<br/>az Úr jóságát<br/>az élőknek földjén!<br/>Várjad az Urat, légy erős,<br/>bátorkodjék és várjad az U-rat!</lines>
    </verse>
  </lyrics>
