<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-12-22T14:04:14">
  <properties>
    <titles>
      <title>Jöjj Szentlélek</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>Jöjj Szentlélek,<br/>Tölts el kérlek,<br/>Járd át ma is,<br/>Az életem.</lines>
    </verse>
    <verse name="v1b">
      <lines>Te vagy az út,<br/>Te vagy az igazság,<br/>Te vagy az élet,<br/>Benned remélek.</lines>
    </verse>
  </lyrics>
</song>
