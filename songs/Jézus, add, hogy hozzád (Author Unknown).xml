      <title>Jézus, add, hogy hozzád</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Éneklő Egyház" entry="544"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Jézus, add, hogy hozzád térjek,<br/>veled haljak, veled éljek.</lines>
    </verse>
    <verse name="v2">
      <lines>Kínodat szánom, vétkemet bánom<br/>kereszted mellett sírván.</lines>
    </verse>
  </lyrics>
