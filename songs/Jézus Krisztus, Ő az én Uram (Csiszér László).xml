  <lyrics>
    <verse name="v1">
      <lines>Jézus Krisztus, Ő az én Uram,<br/>halleluja!</lines>
    </verse>
    <verse name="c1">
      <lines>Ő így szeret, Ő így szeret,<br/>halleluja!</lines>
    </verse>
    <verse name="v2">
      <lines>Jézus Krisztus az egyetlen Király,<br/>halleluja!</lines>
    </verse>
