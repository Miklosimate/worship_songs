  <properties>
    <titles>
      <title>Ne hagyj el engem, Isten, ne</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Dicsérjétek az Urat!" entry="400b"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Ne hagyj el engem, Isten, ne menj el tőlem,<br/>figyelmezz segítségemre, üdvösségemnek ereje.</lines>
    </verse>
    <verse name="v2">
      <lines>Befogadtuk, Isten, irgalmasságodat<br/>(*)<br/>templomodnak bensejében.</lines>
    </verse>
  </lyrics>
</song>
