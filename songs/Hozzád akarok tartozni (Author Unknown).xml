<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 3.1.1" modifiedIn="OpenLP 3.1.1" modifiedDate="2024-06-01T13:52:15">
  <properties>
    <titles>
      <title>Hozzád akarok tartozni</title>
    </titles>
    <verseOrder>v1 v2</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zöld könyv" entry="386"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hozzád akarok tartozni,<br/>Veled akarok lenni,<br/>Érted akarok dolgozni,<br/>Neked akarok tenni!</lines>
    </verse>
    <verse name="v2">
      <lines>Téged akarlak ismerni,<br/>Feléd akarok menni,<br/>Hozzád akarok érkezni,<br/>Tied akarok lenni!</lines>
    </verse>
  </lyrics>
</song>
