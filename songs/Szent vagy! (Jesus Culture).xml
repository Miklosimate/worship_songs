  <properties>
    <titles>
      <title>Szent vagy!</title>
      <title>Holy</title>
    </titles>
    <verseOrder>v1 c1 b1</verseOrder>
    <authors>
      <author>Jesus Culture</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Egyetlen szó mely méltó,<br/>Amely Hozzád fogható</lines>
    </verse>
    <verse name="c1">
      <lines>Szent vagy, szent vagy, ó Mindenható!</lines>
    </verse>
    <verse name="b1">
      <lines>Nincs más olyan, mint Te, mert Te szent vagy s méltó!</lines>
    </verse>
  </lyrics>
