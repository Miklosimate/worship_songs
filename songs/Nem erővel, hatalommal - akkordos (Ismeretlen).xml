<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-02-17T15:36:20">
  <properties>
    <titles>
      <title>Nem erővel, hatalommal - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>D------G/D--D-----G/D<br/>Nem erővel, hatalommal,</lines>
    </verse>
    <verse name="v1b">
      <lines>--D-------D/F#-----G-<br/>A Te Lelkeddel, ó Uram!</lines>
    </verse>
    <verse name="v1c">
      <lines>--D/F#-Em7---D----A/C#-Hm7<br/>Sebeid által a szív meggyógyul,</lines>
    </verse>
    <verse name="v1d">
      <lines>--G---------A---D-D/F#<br/>Megtört életünk megújul.</lines>
    </verse>
    <verse name="v1e">
      <lines>G-D/F#-Em7-D-A/C#-Hm7<br/>Jöjjön a Földre Lelked tüze,</lines>
    </verse>
    <verse name="v1f">
      <lines>-G-----------A-------G-D<br/>Mindenki dicsérjen örökre!</lines>
    </verse>
  </lyrics>
</song>
