<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-12-22T14:04:06">
  <properties>
    <titles>
      <title>Hiszem, hogy meglátom az Úr</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="49"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Hiszem, hogy meglátom az Úr javait,<br/>* az élők földjén.</lines>
    </verse>
    <verse name="v2">
      <lines>Urunk, Istenünk<br/>* a mennyben állította fel trónusát.</lines>
    </verse>
    <verse name="v3">
      <lines>Az Úr uralkodik,<br/>* magasságos Úr a világ felett.</lines>
    </verse>
  </lyrics>
</song>
