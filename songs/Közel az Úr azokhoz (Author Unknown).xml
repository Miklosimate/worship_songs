<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-12-22T14:04:17">
  <properties>
    <titles>
      <title>Közel az Úr azokhoz</title>
    </titles>
    <verseOrder>v1 v2 v3</verseOrder>
    <authors>
      <author>Author Unknown</author>
    </authors>
    <songbooks>
      <songbook name="Zsoltár - vasárnapok" entry="28"/>
    </songbooks>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Közel az Úr azokhoz,<br/>* akik oltalmát kérik.</lines>
    </verse>
    <verse name="v2">
      <lines>Isten megsegít engem,<br/>* az Úr oltalmazója életemnek.</lines>
    </verse>
    <verse name="v3">
      <lines>Dicsérjétek az Urat,<br/>* aki a szegényt fölemeli.</lines>
    </verse>
  </lyrics>
</song>
