<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2020-12-22T14:03:47">
  <properties>
    <titles>
      <title>Aki utánam jön</title>
    </titles>
    <authors>
      <author>Csiszér László</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1">
      <lines>Aki utánam jön hatalmasabb nálam,<br/>Szentlélekkel és tűzzel fog megkeresztelni!<br/>Jézus Krisztus hatalmas vagy, mindeneknél hatalmasabb!</lines>
    </verse>
  </lyrics>
</song>
