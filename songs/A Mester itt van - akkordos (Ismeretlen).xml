<?xml version='1.0' encoding='UTF-8'?>
<song xmlns="http://openlyrics.info/namespace/2009/song" version="0.8" createdIn="OpenLP 2.4.6" modifiedIn="OpenLP 2.4.6" modifiedDate="2021-02-17T15:08:50">
  <properties>
    <titles>
      <title>A Mester itt van - akkordos</title>
    </titles>
    <authors>
      <author>Ismeretlen</author>
    </authors>
  </properties>
  <lyrics>
    <verse name="v1a">
      <lines>----------A-------------C#m-<br/>A Mester itt van, ma is gyógyít,</lines>
    </verse>
    <verse name="v1b">
      <lines>--------Hm--------------E-----<br/>Jézus gyógyít és szabaddá tesz!</lines>
    </verse>
    <verse name="c1a">
      <lines>F#m------------C#m-------D-----------A------<br/>Ne vidd tovább a terhedet, Ő letörli minden könnyedet,</lines>
    </verse>
    <verse name="c1b">
      <lines>----G-----------D----------A-<br/>S gyászod nagy örömre fordul!</lines>
    </verse>
  </lyrics>
</song>
